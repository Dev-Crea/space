<schemalist gettext-domain="gnome-shell-extensions">
    <schema id="org.gnome.shell.extensions.space"
        path="/org/gnome/shell/extensions/space/">

        <key type="b" name="disk-percent">
            <default>true</default>
            <summary>Display data in percent</summary>
            <description>
                If true, the size disk free in displaying in percent. By default is true.
            </description>
        </key>

    </schema>
</schemalist>
